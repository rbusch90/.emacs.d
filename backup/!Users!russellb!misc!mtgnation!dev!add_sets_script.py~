from dev.setlist import full_setlist
from oracle.models import Card
import db_input

sets_to_add = [
    "H09",
    "PD2",
    "PD3",
    "DRB",
    "V10",
    "BD",
    "BR",
    "D2",
    "DDC",
    "DDD",
    "DDE",
    "DDF",
    "DDG",
    "DDH",
    "DDI",
    "DDJ",
    "FVE",
    "FVL",
    "EVG",
    "FVR"
]

latest = 0

for s in sets_to_add:
    latest = Card.objects.lastest('pk').pk

    db_input.add_cards(s)
    new_cards = Card.objects.filter(pk__gt=latest)
    for c in new_cards:
        c.expansion = full_setlist[s]
        c.save()
