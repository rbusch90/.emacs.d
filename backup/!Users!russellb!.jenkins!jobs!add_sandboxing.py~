#######################################
## Run from Jenkins 'jobs' directory ##
## to copy exemplar job attributes   ##
## to other Paclet jobs.             ##
#######################################

from shutil import copyfile
import os

def replace_xml(exemplar,target):
    exemplar_path = os.path.join(os.getcwd(),"%s/config.xml" % (exemplar))
    target_path = os.path.join(os.getcwd(),"%s/config.xml" % (target))

    if os.path.exists(exemplar_path) and os.path.exists(target_path):
        copyfile(exemplar_path,target_path)

