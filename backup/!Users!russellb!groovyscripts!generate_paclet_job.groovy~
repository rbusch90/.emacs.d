def default_exemplar = "TestPacletData"
def localized_exemplar = "CharacterDataLocalized"
def stubless_exemplar = "MaterialData"


def paclet_jobs = Hudson.instance.getView("WolframAlpha").getView("Paclets").getItems()

def exemplar_name = ""
if(paclet_type=="default paclet job"){
    exemplar_name = default_exemplar
}
else if(paclet_type=="localized paclet job"){
    exemplar_name = localized_exemplar
}
else if(paclet_type=="no stub paclet job"){
    exemplar_name = stubless_exemplar
}

exemplar_job = paclet_jobs.find {
    it.getName() == exemplar_name
}

def new_job = Hudson.instance.copy(exemplar_job,paclet_name)

new_job