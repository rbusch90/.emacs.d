for (j in jobs){
    job = Hudson.instance.getItems().find{it.name==j}

    arr = []
  
    i = 0
    for (e in job.getBuildersList()){
    	if (i != 1 && i != 2){
    	   arr.add(e)
	}
	job.getBuildersList().remove(e)
	i++
    }
    job.getBuildersList().add(new org.jenkinsci.plugins.configfiles.builder.ConfigFileBuildStep(f))
    for (e in arr){
    	job.getBuildersList().add(e)
    }
}