builds = [];
build_description = "";

for (build in builds){
    build.keepLog(true);
    build.setDescription(build_description);
    build.save()
    time = build.getTimestamp();
    def command = "cvs co -D \"${time.getDate()}-${time.getMonth()}-${time.getYear()} " +
    	      "${time.getHours()}:${time.getMinutes()}:${time.getSeconds()} CST\"";

    def exec = command.execute();
    exec.waitFor();
}