<project name='SerialLink' basedir='.'>

	<property name='component' value='SerialLink' />
	<property name='system_id' value='### Must be set by Jenkins ###' />

	<property name='app.name' value='${component}' />
	<property name='inputDir' value='${env.WORKSPACE}' />
	<property name='language' value='### Must be set by Jenkins ###' />

	<property environment='env' />
	<import file='${env.ANT-LIBRARY_HOME}/ant-lib.xml' />

	<!--
		Component (platform-independent)
	-->

	<target name='Component.SerialLink.build'>

		<ant antfile='build.xml'>
			<property name='buildCode' value='${env.BUILD_NUMBER}' />
			<property name='build' value='${files_directory}' />
			<target name='build' />
		</ant>

	</target>

</project>
